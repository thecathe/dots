{
  "scripts": [
    "tut_systemd_boot.sh",
    "launch_vscode_workspace.sh"
  ],
  "configs": [
    "~/.config/awesome/rc.lua",
    "~/.config/awesome/xrandr.lua",
    "~/.config/awesome/awful/hotkeys_popup/keys/init.lua",
    "~/.config/awesome/awful/hotkeys_popup/keys/tags.lua",
    "~/.config/awesome/awful/hotkeys_popup/keys/tmux.lua",
    "~/.config/awesome/awful/hotkeys_popup/keys/firefox.lua",
    "~/.config/flameshot/flameshot.ini",
    "~/.config/alacritty/alacritty.toml"
  ],
  "backups": [
    {
      "path": "./etc/lightdm/lightdm.conf",
      "keep_local": true,
      "cache_global": true
    },
    {
      "path": "~/.tmux.conf",
      "keep_local": true,
      "cache_global": true,
      "ignore": false
    },
    {
      "path": "~/.config/awesome/device_spec.lua",
      "keep_local": true,
      "cache_global": true,
      "ignore": false,
      "never_refresh": true
    },
    {
      "path": "~/.bashrc",
      "keep_local": true,
      "cache_global": true,
      "ignore": false
    },
    {
      "path": "~/.zshrc",
      "keep_local": true,
      "cache_global": true,
      "ignore": false
    },
    {
      "path": "~/.xinitrc",
      "keep_local": true,
      "cache_global": true,
      "ignore": true
    },
    {
      "path": "~/bin/run_dot_manager.sh",
      "keep_local": true,
      "cache_global": true,
      "ignore": true,
      "never_backup": true
    }
  ]
}